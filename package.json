{
"name": "@gonextgames/utils",
    "version": "0.0.2",
    "publishConfig": {
      "registry": "https://registry.npmjs.org/",
      "access": "public"
    },
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "files": [
      "dist",
      "src/**/*.css"
    ],
    "scripts": {
      "build": "esbuild src/index.js --bundle --minify --platform=neutral --format=esm --outdir=dist && esbuild src/index.js --bundle --minify --platform=neutral --format=cjs --outdir=dist",
      "dev": "esbuild src/index.js --bundle --watch --platform=neutral --format=esm --outdir=dist",
      "lint": "eslint src/**/*.{js,jsx}"
    },
    "dependencies": {
      "@aws-sdk/client-dynamodb": "^3.726.1",
      "@aws-sdk/lib-dynamodb": "^3.726.1",
      "axios": "^1.7.9",
      "bcryptjs": "^2.4.3",
      "jsonwebtoken": "^9.0.0",
      "next": "^15.1.4",
      "next-sitemap": "^4.2.3",
      "nodemailer": "^6.9.16",
      "openai": "^4.0.0",
      "react": "^19.0.0",
      "react-dom": "^19.0.0",
      "react-youtube": "^10.1.0",
      "@aws-sdk/client-s3": "^3.726.1",
      "zustand": "^5.0.3"
    },
    "devDependencies": {
      "@types/node": "22.10.5",
      "@types/react": "19.0.6",
      "esbuild": "^0.20.1",
      "postcss": "^8.4.35"
    },
    "peerDependencies": {
      "react": ">=16.8.0",
      "react-dom": ">=16.8.0"
    }
  }