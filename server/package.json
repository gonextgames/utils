{
"name": "@gonextgames/utils-server",
    "version": "0.0.14",
    "publishConfig": {
      "registry": "https://registry.npmjs.org/",
      "access": "public"
    },
    "main": "dist/index.js",
    "module": "dist/index.js",
    "files": [
      "dist"
    ],
    "type": "module",
    "scripts": {
      "build": "rm -rf dist && babel src -d dist --copy-files --extensions \".js,.jsx\"",
      "prepare": "npm run build",
      "dev": "babel src -d dist --copy-files --watch --extensions \".js,.jsx\"",
      "test-exports": "node --experimental-vm-modules -e \"import('@gonextgames/utils-server').then(module => console.log('Exports available:', Object.keys(module))).catch(err => { console.error(err); process.exit(1); })\""
    },
    "dependencies": {
      "@aws-sdk/client-dynamodb": "^3.726.1",
      "@aws-sdk/lib-dynamodb": "^3.726.1",
      "@aws-sdk/client-s3": "^3.726.1",
      "axios": "^1.7.9",
      "bcryptjs": "^2.4.3",
      "jsonwebtoken": "^9.0.0",
      "nodemailer": "^6.9.16",
      "openai": "^4.0.0",
      "next": "^15.1.4"
    },
    "devDependencies": {
      "@babel/cli": "^7.23.9",
      "@babel/core": "^7.23.9",
      "@babel/preset-env": "^7.23.9",
      "@babel/preset-react": "^7.23.9",
      "next": "^15.1.4"
    },
    "engines": {
    "node": ">=20.0.0"
  },
  "peerDependencies": {
    "next": "15.1.4"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  }
}